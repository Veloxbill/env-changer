{
    "name": "veloxbill/env-changer",
    "description": "Laravel env changer for sites",
    "keywords": [
        "laravel",
        "laravel-env",
        "env",
        "environment",
        "variables"
    ],
    "homepage": "https://github.com/Veloxbill/env-changer/",
    "license": "MIT",
    "authors": [
        {
            "name": "Max",
            "email": "max@veloxbill.net",
            "homepage": "https://veloxbill.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^7.4 || ^8.0 || ^8.1 || ^8.2 || ^8.3",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "msztorc\\LaravelEnv\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "msztorc\\LaravelEnv\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "msztorc\\LaravelEnv\\LaravelEnvServiceProvider"
            ],
            "aliases": {
                "LaravelEnv": "msztorc\\LaravelEnv\\LaravelEnvFacade"
            }
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2 || ^3",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^8.0 || ^9.0 || ^10.0 || ^11.0"
    }
}
